rule initParam : (Node, Edge, Node, Edge, Node)
{

    replace {
	    a:Node;
	    b:Node;
	    c:Node;
	    
        a -e:Edge-> b;
        b -f:Edge-> c;
        
        return(a, e, b, f, c);
    }
}

