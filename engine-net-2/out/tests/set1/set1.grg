using set1;

test aWithFoo
{
	na:A\B;

	if { "foo" in na.a; }
}

rule addFooNoEffect()
{
    na:A\B;
	
	modify {
		eval {
			na.a = na.a | set<string>{ "foo" };
		}
	}
}

rule addFoo()
{
    nb:B;
	modify {
		eval {
			nb.b = nb.b | set<string>{ "foo" };
		}
	}
}

test bWithBar
{
	nb:B;
	if { "bar" in nb.a; }
}

rule removeBar()
{
    nb:B;

	modify {
		eval {
			nb.a = nb.a \ set<string>{ "bar" };
		}
	}
}

rule removeBarNoEffect()
{
    nb:B;
	
	modify {
		eval {
			nb.a = nb.a \ set<string>{ "bar" };
		}
	}
}

test anonymousSet
{
	na:A;
	if { na.val in set<string>{ "foo", "argl" }; }
}

test anonymousSetNA
{
	nb:B;
	if { nb.val in set<string>{ "aha","oho","uh?"}; }
}

