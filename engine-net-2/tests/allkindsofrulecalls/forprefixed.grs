new graph "allkindsofrulecalls/rules"

dump set node N color red
dump add node N shortinfotag i
dump set node M color blue

randomseed 0

exec init

exec [for{r; s}]

clear graph

exec init

exec [[for{r; true}, for{s; false}]]

clear graph

exec init

exec <<[[for{r; false}, for{s; true}]]>>

clear graph

quit
