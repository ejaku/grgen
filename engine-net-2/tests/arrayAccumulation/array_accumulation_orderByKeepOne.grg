rule orderAscending : (array<int>, array<int>) {
---
	def ref x:array<int>;
	
	yield { 
		yield x = array<int>[1,3,2].orderAscending();
	}
	
	modify {
	---
		def ref y:array<int>;

		eval {
			yield y = array<int>[1,3,2].orderAscending();
		}
		
		return(x,y);
	}
}

