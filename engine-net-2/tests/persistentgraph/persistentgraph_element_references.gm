node edge graph; // required by persistent graph when node/edge references are employed together with graph references

node class N
{
	n:N;
	e:E;
	o:O;
	g:graph;
	nodeFromOtherwiseUnreachableGraph:N;
}

edge class E
{
	n:N;
	e:E;
	o:O;
	g:graph;
}

class O
{
	n:N;
	e:E;
	o:O;
	g:graph;
}
