rule reachabOutgoingLoop(start:Node, var k:int) : (set<Node>)
{
	modify {
		def ref res:set<Node> = set<Node>{};
		eval {
			for(n:Node in boundedReachableOutgoing(start, k)) {
				res.add(n);
			}
		}
		return(res);
	}
}

