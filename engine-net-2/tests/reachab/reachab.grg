rule reachabOutgoingPure(start:Node) : (set<Node>)
{
	def ref res:set<Node> = reachableOutgoing(start);
	modify {
		return(res);
	}
}

rule reachabOutgoingAdj(start:Node) : (set<Node>)
{
	def ref res:set<Node> = adjacentOutgoing(start);
	modify {
		return(res);
	}
}

