rule s(ref sg:set<Gaggl>)
{
	g1:Gaggl;

	modify {
		g2:Gaggl;

		eval {
			sg = set<Gaggl> { g1 };

			sg = set<Gaggl> { g1 } \ set<Gaggl> { g2 };
