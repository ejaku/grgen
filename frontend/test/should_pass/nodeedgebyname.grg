rule rn : (Node)
{
	-e:Edge->;
	if { nodeByName("$0")==source(e); }
	if { nameof(nodeByName("$0"))=="$0"; }
	
	modify {
		def n:Node;
		eval {
			yield n = nodeByName("$42");
		}
		return(nodeByName("$0"));
		exec ( { yield n = nodeByName(nameof(nodeByName("$42"))) } );
	}
}

rule re : (Edge)
{
	-e:Edge->;
	if { edgeByName("$0")==e; }
	if { nameof(edgeByName("$0"))=="$0"; }
	
	modify {
		def -n:Edge->;
		eval {
			yield n = (Edge)edgeByName("$42");
		}
		return((Edge)edgeByName("$0"));
		exec ( { yield n = (Edge)edgeByName(nameof(edgeByName("$42"))) } );
	}
}

