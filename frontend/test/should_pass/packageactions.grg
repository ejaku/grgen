package Foo
{
	pattern P {
		x:Node -e:Edge-> x;
		if{ incident(x).size()>1 && func() && func2(); }

		modify {
			delete(e);
			x -i:Edge-> y:Node;
			eval{ add(Node); }
			eval{ proc(func() && func2()); }

			exec(t\auto ;> r\f ;> { proc(true); { func() && func2() } } ;> s ;> r2\auto);		
		}
	}
	
	test t {
		x:Node -e:Edge-> x;
		if{ incident(x).size()>1 && func(); }
	} \ auto
	
	rule r {
		x:Node -e:Edge-> x;
		if{ incident(x).size()>1 && func() && func2(); }
		p:P();

		modify {
			delete(e);
			x -i:Edge-> y:Node;
			p();
			eval{ add(Node); }
			eval{ proc(func() && func2()); }

			exec(t\auto ;> r\f ;> { proc(true); { func() && func2() } } ;> s ;> r2\auto);
		}
	}
	
	filter f<r> {
		return;
	}
	
	function func : boolean {
		return(true);
	}
	
	procedure proc(var b:boolean) {
		exec(t\auto ;> r\f ;> { proc(true); { func() && func2() } } ;> s);
		return;
	}
	
	sequence s {
		t\auto ;> r\f ;> { proc(true); { func() && func2() } } ;> s ;> r2\auto
	}
}

package Bar 
{
	pattern P {
		x:Node -e:Edge-> x;
		if{ incident(x).size()>1 && func() && func2(); }

		modify {
			delete(e);
			x -i:Edge-> y:Node;
