actions attrib_014 using attrib_model_001;

rule r {
	pattern {
		ni : NINT;
		nb : NBOOL;
		nf : NFLOAT;
		nd : NDOUBLE;
		ns : NSTRING;
		-ei:EINT->;
		-eb:EBOOL->;
		-ef:EFLOAT->;
		-ed:EDOUBLE->;
		-es:ESTRING->;
	}
	modify {
		eval {
			ni.a = 1;
			nb.a = true;
			nf.a = 1.0;
			nd.a = 1.0;
			ns.a = "";

			ei.a = 2;
			eb.a = false;
			ef.a = 2.0;
			ed.a = 2.0;
			es.a = "Hi!";

			nb.a = nf.a == ef.a;
			nb.a = nf.a != ef.a;
			nb.a = nf.a <  ef.a;
			nb.a = nf.a >  ef.a;
			nb.a = nf.a <= ef.a;
			nb.a = nf.a >= ef.a;
		}
	}
}

