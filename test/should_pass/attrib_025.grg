actions attrib_025 using attrib_model_001;

rule r {
	pattern {
		ni : NINT;
		nb : NBOOL;
		nf : NFLOAT;
		nd : NDOUBLE;
		ns : NSTRING;
		-ei:EINT->;
		-eb:EBOOL->;
		-ef:EFLOAT->;
		-ed:EDOUBLE->;
		-es:ESTRING->;
	}
	modify {
		eval {
			ni.a = 1;
			nb.a = true;
			nf.a = 1.0;
			nd.a = 1.0;
			ns.a = "";

			ei.a = 2;
			eb.a = false;
			ef.a = 2.0;
			ed.a = 2.0;
			es.a = "Hi!";

			ni.a = (int)     ni.a;
			nf.a = (float)   ni.a;
			nd.a = (double)  ni.a;
			ns.a = (string)  ni.a;

			nb.a = (boolean) nb.a;
			ns.a = (string)  nb.a;

			ni.a = (int)     nf.a;
			nf.a = (float)   nf.a;
			nd.a = (double)  nf.a;
			ns.a = (string)  nf.a;

			ni.a = (int)     nd.a;
			nf.a = (float)   nd.a;
			nd.a = (double)  nd.a;
			ns.a = (string)  nd.a;

			ns.a = (string)  ns.a;
		}
	}
}

